{
  "name": "quick-status",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "test:ci": "turbo run test -- --ci",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/node": "^24.0.12",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "version": "1.0.0"
}
